{
  "name": "trad-ia-backend",
  "version": "2.0.0",
  "description": "Backend Express para Trad.ia Trading Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
