  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "express-rate-limit": "^6.8.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.0",
    "dotenv": "^16.3.1",
    "ws": "^8.13.0",
    "axios": "^1.5.0",
    "node-cron": "^3.0.2",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/pg": "^8.10.0",
    "@types/node": "^20.4.5",
    "@types/ws": "^8.5.5",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "jest": "^29.6.1",
